{
  "name": "jatour-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:server": "cd server && nest start --watch",
    "build": "next build",
    "build:server": "nest build",
    "start": "next start",
    "start:server": "node dist/server/main.js",
    "lint": "eslint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "cross-env dotenv_config_path=.env.local prisma migrate dev",
    "prisma:studio": "cross-env dotenv_config_path=.env.local prisma studio",
    "prisma:seed": "cross-env NODE_ENV=development ts-node -r dotenv/config prisma/seed.ts dotenv_config_path=.env.local",
    "prisma:create-user": "cross-env ts-node -r dotenv/config prisma/create-user.ts dotenv_config_path=.env.local",
    "prisma:verify-user": "cross-env ts-node -r dotenv/config prisma/verify-user.ts dotenv_config_path=.env.local",
    "prisma:check-users": "cross-env ts-node -r dotenv/config prisma/check-users.ts dotenv_config_path=.env.local",
    "prisma:reset-demo": "cross-env ts-node -r dotenv/config prisma/reset-demo-password.ts dotenv_config_path=.env.local"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-leaflet": "^4.2.1",
    "swr": "^2.3.6",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.5",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.5",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^10.4.5",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^5.0.2",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^10.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "prisma": "^5.20.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
